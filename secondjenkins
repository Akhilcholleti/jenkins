pipeline {
    agent any
    environment {
    PATH="/opt/apache-maven-3.6.3/bin:$PATH"
    }
    stages {
        stage('git') {
            steps {
                git branch: 'main', credentialsId: 'github77', url: 'https://github.com/Akhilcholleti/devops-.git'
        }
        }
        stage('Build') {
            steps {
                sh "mvn clean install"
            }
        }
        }
        }
    
