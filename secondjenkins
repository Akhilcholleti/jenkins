pipeline {
    agent any

    stages {
        stage('git') {
            steps {
                git branch: 'main', credentialsId: 'github77', url: 'https://github.com/Akhilcholleti/devops-.git'
        }
        }
        stage('Build') {
            steps {
                sh "mvn clean install"
            }
        }
        }
        }
    
