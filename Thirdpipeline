pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
               git branch: 'main', credentialsId: 'github77', url: 'https://github.com/Akhilcholleti/devops-.git'
            }
        }
        stage('Build') {
            steps {
                dir("/var/lib/jenkins/workspace/PipelineThird") {
                sh 'mvn -B -DskipTests clean package'
                }
            }
            stage('Test') {
            steps {
                sh 'mvn test'
            }
            
    }
}
}
